    "import commonsense.conceptnet as kb\n",
    "from commonsense.logical_classes import to_data_frame\n",
    "\n",
    "cn = kb.ConceptNet()\n",
    "\n",
    "facts = cn.search_all(\"fruit\")\n",
    "facts"
   ]
